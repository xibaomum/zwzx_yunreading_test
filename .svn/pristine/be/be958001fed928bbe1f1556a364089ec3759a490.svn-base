<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="名家大作测试用例" preserve-order="true">
	<listeners>
		<listener class-name="main.util.RetryListener"></listener>
		<listener class-name="main.util.TestNGListener"></listener>
	</listeners>
	<test name="1.首页获奖文学单本">
		<parameter name="alterMsg" value="图书详情"></parameter>
		<classes>
			<class name="Test.Baidu.MJDaZTest">
				<methods>
					<include name="testHJWXSingleBook"></include>
					<!-- <include name="testGetCurUrl"></include> -->
				</methods>
			</class>
		</classes>
	</test>

	<test name="2.首页获奖文学【更多】按钮">
