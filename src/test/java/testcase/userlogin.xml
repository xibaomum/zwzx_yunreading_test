<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="注冊|登录|退出系统测试用例" preserve-order="true">
        <parameter name="username" value="test567"></parameter>
		<parameter name="password" value="111111"></parameter>

<test name="1.正常注释">
		<classes>
			<class name="common.test.LoginOutTest">
				<methods>
					<include name="testRegister"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="2.正常登录">
		<classes>
			<class name="common.test.LoginOutTest">
				<methods>
					<include name="testLogin"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="3.退出">
		<classes>
			<class name="common.test.LoginOutTest">
				<methods>
					<include name="testLoginOut"></include>
				</methods>
			</class>
		</classes>
	</test>

</suite>