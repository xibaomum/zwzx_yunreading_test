<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="排行榜测试用例" preserve-order="true">
	<test name="1.首页图书单本">
		<parameter name="currentUrl" value="http://nqcx-test.w.chineseall.cn/book/detail/CIg5g/1353057"></parameter>
		<classes>
			<class name="Test.Baidu.PHBTest">
				<methods>
					<include name="testSingleBook"></include>
					<include name="testGetCurUrl"></include>
				</methods>
			</class>
		</classes>
	</test> 
	
	<test name="2.首页图书【更多】按钮">
		<parameter name="alterMsg" value="图书详情"></parameter>
		<classes>
			<class name="Test.Baidu.PHBTest">
				<methods>
					<include name="testMoreBook"></include>
				</methods>
			</class>
		</classes>
	</test> 
	
	<test name="3.首页听书单本">
		<parameter name="currentUrl" value="http://nqcx-test.w.chineseall.cn/audio/detail/33HKj/47"></parameter>
		<classes>
			<class name="Test.Baidu.PHBTest">
				<methods>
					<include name="testSingleListen"></include>
					<include name="testGetCurUrl"></include>
				</methods>
			</class>
		</classes>
	</test> 
	
	<test name="2.首页听书【更多】按钮">
		<parameter name="alterMsg" value="听书详情"></parameter>
		<classes>
			<class name="Test.Baidu.PHBTest">
				<methods>
					<include name="testMoreListen"></include>
				</methods>
			</class>
		</classes>
	</test> 
</suite>