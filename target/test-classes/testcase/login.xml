<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="登录|退出系统测试用例" preserve-order="true">



	<test name="1.正常登录">
		<parameter name="username" value="testcjing"></parameter>
		<parameter name="password" value="111111"></parameter>
		<classes>
			<class name="common.test.LoginOutTest">
				<methods>
					<include name="testLogin"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="2.退出">
		<classes>
			<class name="common.test.LoginOutTest">
				<methods>
					<include name="testLoginOut"></include>
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="3.账户为空">
	<parameter name="usernameMsg" value="账号不能为空!"></parameter>
		<classes>
			<class name="common.test.LoginOutTest">
				<methods>
					<include name="testAssertUsername"></include>
				</methods>
			</class>
		</classes>
	</test>
	
		
	<test name="4.密碼为空">
	<parameter name="username" value="testcjing"></parameter>
	<parameter name="passwordMsg" value="密码不能为空!"></parameter>
		<classes>
			<class name="common.test.LoginOutTest">
				<methods>
					<include name="testAssertPassword"></include>
				</methods>
			</class>
		</classes>
	</test>
	
		
	<test name="5.用户名不存在">
	<parameter name="username" value="testabc"></parameter>
	<parameter name="password" value="111111"></parameter>
	<parameter name="usernameUableMsg" value="登录名不存在"></parameter>
		<classes>
			<class name="common.test.LoginOutTest">
				<methods>
					<include name="testAssertUsernameUnable"></include>
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="6.密码错误">
	<parameter name="username" value="testcjing"></parameter>
	<parameter name="password" value="123456"></parameter>
	<parameter name="passworderrMsg" value="密码错误"></parameter>
		<classes>
			<class name="common.test.LoginOutTest">
				<methods>
					<include name="testAssertPasswordErr"></include>
				</methods>
			</class>
		</classes>
	</test>

</suite>